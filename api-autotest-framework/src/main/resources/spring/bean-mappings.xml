<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:component-scan
		base-package="api.autotest.framework" />
	<context:annotation-config />

	<bean id="autowiredAnnotationBeanPostProcessor"
		class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"></bean>
	<bean id="restClient"
		class="api.autotest.framework.rest.RestClient"></bean>
	<bean id="restClientUtils"
		class="api.autotest.framework.rest.RestClientUtils"></bean>
</beans>